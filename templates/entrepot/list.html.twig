{% extends 'back.base.html.twig' %}




{% block body %}
<div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Entrepots</h1>
                    <p class="mb-4">Entrepots <a target="_blank"
                            href="https://datatables.net"></a>.</p>

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary">Entrepots</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                           <th>Entrepotid</th>
                <th>Nome</th>
                <th>Adressee</th>
                <th>Capacitee</th>
                <th>Statute</th>
                <th>actions</th>
                                        </tr>
                                    </thead>
                              
                                    <tbody>
                                     {% for entrepot in entrepots %}
                                       <tr>
                <td>{{ entrepot.entrepotid }}</td>
                <td>{{ entrepot.nome }}</td>
                <td>{{ entrepot.adressee }}</td>
                <td>{{ entrepot.capacitee }}</td>
                <td>{{ entrepot.statute }}</td>
                <td>
                    <a href="{{ path('app_entrepot_show', {'entrepotid': entrepot.entrepotid}) }}">show</a>
                    <a href="{{ path('app_entrepot_edit', {'entrepotid': entrepot.entrepotid}) }}">edit</a>
                </td>
            </tr>
                                {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}        
                                       
                                    </tbody>
                                </table>
                                <a href="{{ path('app_entrepot_new') }}">Create new</a>
                            </div>
                        </div>
                    </div>

                </div>
                {% endblock %}